<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yixunlian.mapper.ExtractprojectMapper">
    <resultMap id="BaseResultMap" type="com.yixunlian.pojo.Extractproject">
        <id column="extractproject_id" jdbcType="VARCHAR" property="extractprojectId"/>
        <result column="project_name" jdbcType="VARCHAR" property="projectName"/>
        <result column="project_rate" jdbcType="DECIMAL" property="projectRate"/>
        <result column="project_money" jdbcType="DECIMAL" property="projectMoney"/>
        <result column="activity_id" jdbcType="VARCHAR" property="activityId"/>
        <result column="created" jdbcType="TIMESTAMP" property="created"/>
        <result column="updated" jdbcType="TIMESTAMP" property="updated"/>
    </resultMap>
    <sql id="Base_Column_List">
    extractproject_id, project_name, project_rate, project_money, activity_id, created, 
    updated
  </sql>

    <select id="queryProjectRateSumByActivityId" parameterType="java.lang.String" resultType="java.math.BigDecimal">
        select sum(project_rate)
        from td_extractproject
        where activity_id =#{activityId}
    </select>

</mapper>